`timescale 1 ps/ 1 ps
module pfd_vlg_vec_tst();
// constants                                           
// general purpose registers
reg a;
reg b;
// wires                                               
wire down;
wire up;

// assign statements (if any)                          
pfd i1 (
// port map - connection between master ports and signals/registers   
	.a(a),
	.b(b),
	.down(down),
	.up(up)
);
initial 
begin 
#1000000 $finish;
end 

// a
initial
begin
	a = 1'b1;
	# 10000;
	repeat(24)
	begin
		a = 1'b0;
		a = #20000 1'b1;
		# 20000;
	end
	a = 1'b0;
	a = #20000 1'b1;
end 

// b
always
begin
	b = 1'b0;
	b = #20000 1'b1;
	#20000;
end 
endmodule

